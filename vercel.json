{
  "rewrites": [
    {
      "source": "/article/:id",
      "destination": "/article.html"
    },
    {
      "source": "/pro",
      "destination": "/pro.html"
    },
    {
      "source": "/vault",
      "destination": "/vault.html"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=60, stale-while-revalidate=300"
        }
      ]
    }
  ]
}